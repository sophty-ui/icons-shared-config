    "prepare:vscode": "vscode-shared-config",
    "release": "run-s prepare lint build generate"
  },
  "dependencies": {
    "standard-shared-config": "^5.1.2"
  },
  "devDependencies": {
    "@tagproject/base-shared-config": "^4.0.6",
